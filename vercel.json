{
  "version": 2,
  "builds": [
    {
      "src": ".",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "cmd": "npm run build",  
        "output": "/dist/running-tools"
      }
    }
  ],
  "routes": [
    {
      "src": "/en/(.*)",
      "dest": "/dist/running-tools/en-US/$1"
    },
    {
      "src": "/br/(.*)",
      "dest": "/dist/running-tools/br/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/running-tools/en-US/$1"
    }
  ]
}
